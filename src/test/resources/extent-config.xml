<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>
		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>
		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>http</protocol>
		<!-- title of the document -->
		<documentTitle>Project 2024</documentTitle>
		<!-- report name - displayed at top-nav -->
		<reportName>Project 2024 Q1</reportName>
		<!-- location of charts in the test view -->
		<!-- top, bottom -->
		<testViewChartLocation>bottom</testViewChartLocation>
		<!-- custom javascript -->
		<scripts>
<![CDATA[ $(document).ready(function() { }); ]]>
		</scripts>
		<!-- custom styles -->
		<styles>
<![CDATA[ ]]>
		</styles>
	</configuration>
</extentreports>